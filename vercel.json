{
    "builds": [
        {
            "src": "backend/config/wsgi.py",
            "use": "@vercel/python",
            "config": { 
                "maxLambdaSize": "15mb", 
                "runtime": "python3.9",
                "includeFiles": "domain/**" 
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "backend/config/wsgi.py"
        }
    ]
}